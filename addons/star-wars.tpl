[+takeback-filelist.css]
.part1 {
    position: fixed;
    width: 100%;
    height: 100%;
    perspective: 64em;
    perspective-origin: 50% 0%;
}
.part1 main {
    transform: rotateX(45deg);
    position: fixed;
    width: 100%;
    height: 100%;
    overflow: auto;
}
.part1 main table#files {
    margin-top: 16em;
    margin-bottom: 32em;
}
